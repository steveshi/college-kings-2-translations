    # aut "Oscar."
    aut "奥斯卡。"

# game/v3/scene52.rpy:866
translate chinese v3s52_4ce55b01:

    # de "Oscar, hm? Seems to fit. You're happy with it?"
    de "奥斯卡，嗯? 看起来很合身，你满意吗？"

# game/v3/scene52.rpy:871
translate chinese v3s52_86568147:

    # oscar_dog "Arf!"
    oscar_dog "汪！"

# game/v3/scene52.rpy:876
translate chinese v3s52_43785e33:

    # de "Very well. What paperwork do I need to sign?"
    de "很好，我需要签什么文件？"

# game/v3/scene52.rpy:881
translate chinese v3s52_6ec631fa:

    # aut "Um, paperwork?"
    aut "文书工作？"

# game/v3/scene52.rpy:886
translate chinese v3s52_8e196a69:

    # de "To adopt him?"
    de "收养他？"

# game/v3/scene52.rpy:891
translate chinese v3s52_b6d8096c:

    # aut "Oh, sorry! You want to adopt him? Just like that? I mean-"
