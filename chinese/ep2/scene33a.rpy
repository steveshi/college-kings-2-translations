# game/ep2/scene33a.rpy:240
translate chinese ep2_s33a_3114fc2f:

    # la "Tomorrow work for you?"
    la "明天可以吗？"

# game/ep2/scene33a.rpy:245
translate chinese ep2_s33a_9defc31a:

    # na "Sounds good. Text me the details?"
